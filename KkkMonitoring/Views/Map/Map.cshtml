
@{
    Layout = "~/Views/MasterPages/KkkMasterPage.cshtml";
    ViewBag.Title = "Map page";
}

@section scriptsArea
{
    <script src="~/Scripts/libs/openlayers3/ol3.js"></script>
    <script src="~/Scripts/libs/jsTree/jstree.min.js"></script>
    <script src="~/Scripts/modules/OLMap.js"></script>
    <script src="~/Scripts/modules/TreeViewModule.js"></script>
    <script src="~/Scripts/page-scripts/map.js"></script>
    <script src="~/Scripts/jquery-2.2.4.min.js"></script>
    <!--Reference the SignalR library. -->
    <script src="~/Scripts/jquery.signalR-2.2.1.min.js"></script>
    <!--Reference the autogenerated SignalR hub script. -->
    <script src="~/signalr/hubs"></script>
    <script src="~/Scripts/page-scripts/parameterSocket.js"></script>
}

@section cssArea
{
    <link href="~/Content/jsTree/themes/default-dark/style.min.css" rel="stylesheet" type="text/css"/>
    <link href="~/Content/ol.css" rel="stylesheet" type="text/css" />
}

@section contentArea {
    <div id="map-wrap">
        <div id="Map"></div>    
    </div>
    
}

@section sidebarArea
{
    <div id="treeView"></div>
}